{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Information",
      "FacebookFnApp": "Information"
    }
  },
  "extensions": {
    "serviceBus": {
      "maxConcurrentCalls": 1,
      "prefetchCount": 0,
      "autoComplete": false,
      "maxAutoRenewDuration": "00:05:00"
    }
  }
}